{
  "expo": {
    "name": "fitcoin",
    "slug": "fitcoin",
    "scheme": "fitcoin",
    "owner": "nbekdev",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "dark",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.nbekdev.fitcoin",
      "infoPlist": {
        "NSPhotoLibraryUsageDescription": "Чтобы загрузить изображение, необходимо открыть галерею!",
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "package": "com.nbekdev.fitcoin",
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "bundler": "metro",
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-web-browser",
      "expo-video"
    ],
    "extra": {
      "router": {},
      "eas": {
        "projectId": "78e98d57-1868-4ba3-8647-0a7d2a447141"
      }
    },
    "runtimeVersion": "1.0.0",
    "updates": {
      "url": "https://u.expo.dev/78e98d57-1868-4ba3-8647-0a7d2a447141"
    }
  }
}
